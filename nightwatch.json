{
  "src_folders": "tests",
  "output_folder": "reports",
  "page_objects_path": "objects",
  "custom_commands_path": "functions",
  "globals_path": "globalVariables.js",
  "live_output": false,

  "test_settings": {
    "default": {
      "launch_url": "http://web",
      "screenshots": {
        "enabled": false
      },
      "desiredCapabilities": {
        "browserName": "firefox",
        "marionette": true
      },
      "test_workers": true,
      "detailed_output": true
    },
    "reporting": {
      "launch_url": "http://web",
      "screenshots": {
        "enabled": false,
        "on_failure": true,
        "on_error": true,
        "path": "screenshots"
      },
      "desiredCapabilities": {
        "browserName": "firefox",
        "marionette": true
      },
      "test_workers": false,
      "detailed_output": false
    }
  }
}
